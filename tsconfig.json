{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "vite-plugin-vue-layouts-next": ["./src/index.ts"],
      "vite-plugin-vue-layouts-next/*": ["./*"]
    }
  },
  "exclude": [
    "**/dist",
    "**/node_modules"
  ]
}
